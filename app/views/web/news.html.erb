<h4>Teated</h4>
<div class="row">
    <% @news.each do |news| %>
    <div class="col-sm-12 news">
        <h5><%= news.heading %></h5>
        <p id="created_at">Avaldatud <%= news.created_at.strftime("%d.%m.%Y") %></p>
        <p><%= raw(news.content)%></p>
        <p id="border"></p>
    </div>
<% end %>
</div>